{
	"name": "__MSG_extName__",
	"version": "0.1",
	"manifest_version": 2,
	"description": "__MSG_extDesc__",
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.4.1.min.js","content.js"]
      }
    ],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"web_accessible_resources": [
    "modal.html"
  ],
	"default_locale": "en",
	"options_page": "options.html",
	"permissions": [
		"contextMenus",
		"downloads",
		"downloads.open",
		"activeTab"
	]
}
